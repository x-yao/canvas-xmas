;(function($){

    var Analytics = function(config){
        var _self = this;
        _self.config = $.extend({}, Analytics.config, config);
        _self._init();
    };

    Analytics.config = {
    	gaTimerInterval: 3000
    };

    $.extend(Analytics.prototype, {
    	_init: function(){
    		var _self = this;
    		// 待发列表;
    		_self._gaList = [];
    		// 计时器
    		_self._gaTimer = null;
    	},
    
    	// 用户点击
    	click: function(label){
    		var _self = this;
    		_self.send({
    			label: label
    		});
    	},
    	// 逻辑触发
    	trigger: function(label){
    		var _self = this;
    		_self.send({
				category: 'js',
				action: 'trigger',
    			label: label
    		});
    	},
    	// 页面到达
    	page: function(label){
    		var _self = this;
    		_self.send({
				category: 'js',
				action: 'page',
    			label: label
    		});
    	},
    
    	// 发送错误埋点
    	error:function(msg, isFatal){
    		var _self = this;
    		_self.send({
				category: 'jsError',
				action: isFatal ? 'error' : 'warning',
    			label: msg || 'error'
    		});
    		/*ga && ga('send', 'exception', {
				'exDescription': msg || 'error',
				'exFatal': !!isFatal
			});*/
    	},
    	
    	// 带重发机制的事件埋点
    	send: function(cfg){
    		var _self = this;
    		if(window.ga){
    			_self._send(cfg);
    		}else{
    			_self._gaList.push(cfg);
    			// 启动timer
    			if(!_self._gaTimer){
    				_self._gaTimer = window.setInterval(_self._gaHandler(), _self.config.gaTimerInterval);
    			}
    		}
   	
    	},
    	_gaHandler: function(){
    		var _self = this;
    		return function(){
				if(ga){
					// 清除timer
					window.clearInterval(_self._gaTimer);
					_self._gaTimer = null;
					// 重发ga
					$.each(_self._gaList, function(i, gaItem){
						_self._send(gaItem);
					});
				}	
    		}
    	},
    	// 发送事件埋点
        _send:function(cfg){
            var _self = this,
            	config = $.extend({
					category: 'button',
					action: 'click',
					label: 'default',
					value: 1
            	}, cfg);
			ga && ga('send', 'event', config.category, config.action, config.label, config.value);
        }
        
    });

    if(!window.KUI){
        window.KUI = {};
    }
    KUI.Analytics = Analytics;

})(Zepto);
