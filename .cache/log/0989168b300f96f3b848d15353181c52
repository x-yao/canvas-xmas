var kop = new KUI.KOP();
var risk = new KUI.RiskControl({
	umidApi:'lj.m.v.h5.getApplyConfig'
});
var _interval_msg,
	flag = true,
	_is_wx = false;
	
var coupon = {
	uuid:'',
	umid:'',
	need_verify_code:0,
	// init:function(){

	// },
	//获取优惠券
	fetch_coupon:function(){
		//var self = this;
		var umid = risk.getUmid(),
			ua = risk.getUa();
		if(!coupon.validate_data()) return;
		var pmob = $('#J_pmob').val(),
			verify_code = $('#J_verify_code').val();
//		console.log("> fetch_coupon");
		// 从API 获取页面数据
		kop.send('lj.m.v.h5.applyVoucher', {
			'mob' : pmob,
			'verifyCode':verify_code,
//			'uuid': coupon.uuid,
			'umid': umid,
			'ua'  : ua,
//			'ua'  : 'gfasrfg',
			'activityId':activityId
		}, null, null, function(data){
//			console.log("> success - data::"+data);
			//_temp_suc = '$${money}元代金券已经放置您的账户，有效期$${day}天';
			data.mobile = pmob;
			//$('#J_result_txt').html(juicer(temp_suc, data));
			//$('#J_total').text(data.money);
//			coupon.show_verify_field();return;
			coupon.show_coupon_result(200,data);
			//防止多次提交
			$('#J_coupons_open').removeClass('disable');
		}, function(err, msg){
			switch(err){
				case 810017:
					coupon.show_verify_field();
					break;
				case 810011:
					$('#J_result_txt').html(arrl_err_msg[0]).addClass('on');
					break;
				case 810016:
					$('#J_result_txt').html(arrl_err_msg[1]).addClass('on');
					break;
				case 810001:
				case 810002:
				case 810004:
					$('#J_result_txt').html(arrl_err_msg[2]).addClass('on');
					break;
				case 810003:
					$('#J_result_txt').html(arrl_err_msg[3]);
					break;
				case 810010:
					$('#J_result_txt').html(arrl_err_msg[4]).addClass('on');
					break;
				case 810015:
					$('#J_result_txt').html(arrl_err_msg[5]);
					break;
				case 810014:
					$('#J_result_txt').html(arrl_err_msg[6]);
					break;
				case 'abort':
					toast("网络异常，请稍后再试");
					break
				default:
					$('#J_result_txt').html(arrl_err_msg[0]).addClass('on');
					break;
			}
			// switch(err){
			// 	case 810011:
			// 		$('#J_fail_face').addClass('face-d');
			// 		$('#J_fail_txt').text('请稍后再试');
			// 		break;
			// 	case 810016:
			// 	case 810003:
			// 	case 810010:
			// 	case 810015:
			// 		$('#J_fail_face').addClass('face-b');
			// 		$('#J_fail_txt').text('很遗憾哦~');
			// 		break;
			// 	case 810018:
			// 		toast(msg);
			// 		break;
			// 	case 810001:
			// 	case 810002:
			// 	case 810004:
			// 		$('#J_fail_face').addClass('face-c');
			// 		$('#J_fail_txt').text('别着急哦~');
			// 		break;
			// 	case 810014:
			// 		$('#J_fail_face').addClass('face-a');
			// 		$('#J_fail_txt').text('别贪心哦~');
			// 		break;
			// 	default:
			// 		$('#J_fail_face').addClass('face-d');
			// 		$('#J_fail_txt').text('请稍后再试');
			// 		break;
			// }
			if(err != 810017 && err != 810018 && err != 'abort'){
				coupon.show_coupon_result(err);
			}
			//防止多次提交
			$('#J_coupons_open').removeClass('disable');
		});
	},
	//发送验证码到乘客号码
	resend_cfm_msg: function(pmob){
		// 从API 获取页面数据
		kop.send('lj.m.v.h5.getVerifyCode', {
			'mob' : pmob,
			'umid' : coupon.umid
		}, null, null, function(data){
			//console.log(data);
			//停止倒计时
			coupon.resend_cfm_msg_loading();
		}, function(err, msg){
			console.log(err);
			toast(msg);
		});
	},
	//验证码倒计时
	resend_cfm_msg_loading: function(_c_start){
          var c_start = 29;
          if(_c_start){
               c_start = _c_start;
          }
          $('#J_btn_c_resend').addClass('disable').attr('disabled', 'disabled');
          $('#J_btn_c_resend').text(c_start+'秒后重发');
          if(_interval_msg){
          }else {
               _interval_msg = setInterval(function(){
                    //console.log('*** resend_cfm_msg_loading -> _interval_msg:'+_interval_msg);
                    if(c_start > 0){
                    	c_start --;
                    	$('#J_btn_c_resend').text(c_start+'秒后重发');
//                  	storage.set('_c_start', c_start);
//                  	console.log(c_start+'秒后重发');
                    }else{
                    	$('#J_btn_c_resend').text('获取验证码');
                         clearInterval(_interval_msg);_interval_msg = undefined;
                         $('#J_btn_c_resend').removeClass('disable').removeAttr('disabled');
                         console.log('*** the confirm loading is end!');
                    }
               }, 1000);
          }
     },
     //验证手机号码
	 validate_phone:function(mob){
		var str=mob;
        var regPartton=/1[3-8]+\d{9}/;
        if (!str || str == null){
            toast("请输入手机号码" );
            return false;
        } else if (!regPartton.test(str)){
            toast("请输入正确的手机号码");
            return false;
        } else {
            return true;
        }
  	},
  	validate_data:function(){
  		var pmob = $('#J_pmob').val();
		if(!coupon.validate_phone(pmob)) return false;
		if(coupon.need_verify_code){
			var _verify_code = $('#J_verify_code').val();
			if(_verify_code && _verify_code.length == 4){
				return true;
			}else{
				toast('请输入正确的验证码');
				return false;
			}
		}
		return true;
  	},
  	//显示提示分享
  	show_share_note:function(){
  		msg_shade(true);
  		var _share_note = '<div class="share-note" id="J_share_note"></div>';
  		$('body').append(_share_note);
  		$('#J_msg_shade, #J_share_note').click(function(){
  			msg_shade(false);
			$('#J_share_note').remove();
		})
  	},
  	show_rule:function(){
  		//show J_coupon_rule
  		msg_shade(true);
		$('#J_coupon_rule').show();
		
		//fit rule box
		dialog_re_adapt();
		setTimeout(function(){
			dialog_re_adapt();
			setTimeout(function(){
				dialog_re_adapt();
			},500);
		},500);
		
		//bind close action
  		$('.coupon-rule').delegate('#J_rule_close','click',function(){
  			msg_shade(false);
  			$('#J_coupon_rule').hide();
  		});
  	},
  	show_coupon_result:function(code,data){
		$('#J_result_box').removeClass("hide");
		$('#J_exc_form,#J_coupons_open').addClass("hide");
		$('.intro-link').remove();
		//$('#J_btn_share').show();
		if(code == 200){
			$('body').trigger('updateShare',data);
			//_is_wx ? $('#J_btn_share').show() : '';
			// setTimeout(function(){
			// 	$('#J_coupons_box_suc').addClass('on');
			// },1500);
		}else{
			//$('#J_btn_share').hide();
			setTimeout(function(){
				$('#J_coupons_box_fail').addClass('on');
			},1500);
		}
  	},
  	show_verify_field:function(){
  		coupon.need_verify_code = 1;
  		$('#J_exc_open').addClass('bigger');
  		$('.exc-box .logo-box').addClass('on');
  		$('#J_verify_filed').show();
  		setTimeout(function(){
  			$('#J_btn_c_resend').click();
  		},10)
  	},
  	show_getcoupon:function(){
  		$('#J_exc_form').removeClass("hide");
  		$('#J_first_open').addClass("hide");
  	}
}

//判断是否为苹果,元素失去焦点隐藏iphone的软键盘
var isIPHONE = navigator.userAgent.toUpperCase().indexOf('IPHONE') != -1;
var objBlur = function(id, time) {
// console.log("> call objBlur.id::"+id);
    if (typeof id != 'string') console.log('objBlur()参数错误');
    var obj = document.getElementById(id),
    time = time || 300,
    docTouchend = function(event) {
        if (event.target != obj) {
            setTimeout(function() {
                obj.blur();
                document.removeEventListener('touchend', docTouchend, false);
            },
            time);
        }
    };
    if (obj) {
        obj.addEventListener('focus',
        function() {
            document.addEventListener('touchend', docTouchend, false);
        },
        false);
    } else {
//      throw new Error('objBlur()没有找到元素');
  console.log('objBlur()没有找到元素');
    }
},
createObjBlur = function(){
 if (isIPHONE) {
  $('input').each(function(i,ele){
   var _type = $(ele).attr('type');
   var _id = $(ele).attr('id');
   if(_type != 'hidden' && typeof _id != 'undefined'){
//    console.log("id>"+_id);
    var input = new objBlur(_id);
    input = null;
   }
  })
 }
}
//快的客户端软键盘遮盖输入框
if((window.KDShare!=undefined && window.KDShare.share!=undefined)&&!isIPHONE){
	$('#J_exc_form input').on('focus',function(){
		fixInput("focus");
	})
	// .on('blur',function(e){
	// 	if($(e.target).hasClass('no-blur')){
	// 		return false
	// 	}
	// 	fixInput("blur");
	// })
	$('#J_exc_form').on('click',function(e){
		if(!$(e.target).hasClass('no-blur')){
			fixInput("blur");
		}
	})
}
function fixInput(state){
	if(state=='blur'){
		$('.top-img').show();
		$('#J_exc_form').removeClass('fix-input');
		$('.didilogo').show();
	}else{
		$('.top-img').hide();
		$('#J_exc_form').addClass('fix-input');
		$('.didilogo').hide();
	}
}
//toast
$('head').append('<style>#J_msg_shade{position:fixed;width:100%;height:200%;z-index:10000000;top:-100%;left:0}</style>');
$('head').append('<style>#toast{border:1px solid #AAA;font-size:16px;line-height:16px;position:fixed;bottom:30%;left:10%;width:73%;margin-left:0;min-width:20%;max-width:80%;min-height:16px;background-color:rgba(0,0,0,.5);padding:16px 8px;text-align:center;opacity:1;color:rgba(255,255,255,.5);-webkit-transition:opacity .5s ease-out;-moz-transition:opacity .5s ease-out;-ms-transition:opacity .5s ease-out;-o-transition:opacity .5s ease-out;transition:opacity .5s ease-out;-moz-border-radius:5px;-webkit-border-radius:5px;border-radius:5px;z-index:11000000;<!-- -webkit-transition:opacity .5s ease-out;-moz-transition:opacity .5s ease-out;-ms-transition:opacity .5s ease-out;-o-transition:opacity .5s ease-out;transition:opacity .5s ease-out;-moz-border-radius:0;-webkit-border-radius:0;border-radius:0;-->}</style>');
var timeoutCounter;
var hideToast = function() {
	msg_shade(false);
    var toast = document.getElementById("toast");
    document.body.removeChild(toast);
    clearInterval(timeoutCounter);
},
toast = function(message,timer) {
	//当未通过验证弹出toast的情况下触发失焦
	fixInput("blur");
    var toast = document.getElementById("toast");
	timer = timer||2000;
    if (toast == null) {
        var toastHTML = '<div id="toast">' + message + "</div>";
        document.body.insertAdjacentHTML("beforeEnd", toastHTML);
        var d_width = $(document).width(),
        t_width = $('#toast').width();
        $('#toast').css('left', ((d_width - t_width) / 2) + 'px');
    } else {
        toast.innerHTML = message;
//      toast.style.opacity = .6;
    }
    msg_shade(true);
    timeoutCounter = setTimeout(function(){hideToast();}, timer);
},
dialog_re_adapt = function(t) { ! t ? t = 0 : '';
    setTimeout(function() {
        var d_height = $(window).height(),
        e_height = $('.dialog').height();
        $('.dialog').css('top', ((d_height - e_height) / 2) + 'px');
    },t)
},
msg_shade = function(flag) {
        if (flag) { ! $("#J_msg_shade").length && $("body").append("<div id='J_msg_shade'/>");
        } else {
            $("#J_msg_shade").remove();
        }
    };

main();
})
