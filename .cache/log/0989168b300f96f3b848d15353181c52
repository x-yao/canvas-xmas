;(function($) {

	var UA_JS = ['<div id="J_RiskControlContent" style="display:none;"></div>'].join(''),
		UA_ID = 'J_RiskControlContent';

	var RiskControl = function(config){
		var _self = this;
		_self.config = $.extend({}, RiskControl.config, config);
		_self._init();
	};
	RiskControl.config = {
		kopConfig: null,
		umidApi: 'lj.u.c.getRiskToken'
	};
		
	$.extend(RiskControl.prototype, {
		_init: function(){
			var _self = this;
			// 将js加到页面底部
			$('body').append(UA_JS);
			// 初始化kop
			_self.kop = new KUI.KOP(_self.config.kopConfig);
			// 获取umid
			_self.getUmid();
		},

		getUa: function(){
			var _self = this;
			_self.ua = window.ua;
			return window.ua;
		},
		
		getUmid: function(){
			var _self = this,
				umid = _self.umid;
			if(umid){
				return umid;
			}else{
				_self._getUmid();
				return null;
			}
		},		
		_getUmid: function(){
			var _self = this;
		
			_self.kop.send(_self.config.umidApi, {}, null, null, function(data){
				_self.umid = data.riskToken || data.umid ||  null;
				_self._connetAlipay(_self.umid);
			}, function(err, msg){
				console.log(err + ': ' + msg);
			});
		},
		_connetAlipay: function(umid){
		    var _self = this,
		    	properties = ['innerWidth', 'innerHeight', 'outerWidth', 'outerHeight', 'screenX', 'screenY', 'availWidth', 'availHeight', 'colorDepth',  'width', 'height'],
		    	values = [],
		    	method = window.encodeURIComponent || window.escape,
		    	isApple = navigator.userAgent.indexOf('AppleWebKit') > -1 && navigator.vendor && navigator.vendor.indexOf('Apple') > -1,
		    	param,
		    	render,
		    	url;
		    	
			$.each(properties, function(i, p){
				values.push(window[p] || '-');
			});
		    	
			param = method(values.join('|'));
			render = 'https://ynuf.alipay.com/dc.htm?token=' + umid;

		    if(isApple) {
		         render = 'https://ynuf.alipay.com/la.htm?token=' + umid;
		     }
		     
		     url = render + '&jsInfo=' + param;
		     
		     if(isApple)  {
		     	$('#' + UA_ID).html('<iframe src=' + url + ' width="0" height="0" frameborder="0"/>');
		     }else{
		     	$('#' + UA_ID).html('<img src=' + url + ' width="0" height="0" frameborder="0"/>');
		     }
		}
		
	});
	
    
    if(!window.KUI){
    	window.KUI = {};
    }
    KUI.RiskControl = RiskControl;
    
