var browser={
	versions:function(){
	    var u = navigator.userAgent, app = navigator.appVersion;
	    return { //
		    ios: !!u.match(/\(i[^;]+;( U;)? CPU.+Mac OS X/), //ios
		    android: u.indexOf('Android') > -1 || u.indexOf('Linux') > -1 //androiduc
        };
    }()
}
//storage
if (window.localStorage) {
    storage = {
        get: function(k, parse) {
            var v = localStorage.getItem(k);
            if (parse) {
                v = JSON.parse(v);
            }
            return v;
        },
        set: function(k, v) {
            if (typeof v !== "string") {
                v = JSON.stringify(v);
            }
            localStorage.setItem(k, v);
        },
        remove: function(k) {
            localStorage.removeItem(k);
        }
    };
} else {
    storage = {
        get: function(k, parse) {
            var v = getCookie(k);
            if (parse) {
                v = JSON.parse(v);
            }
            return v;
        },
        set: function(k, v) {
            if (typeof v !== "string") {
                v = JSON.stringify(v);
            }
            setCookie(k, v);
        },
        remove: function(k) {
            delCookie(k);
        }
    };
}

var _a_month = 2592e6;
var length_add = 0;

var setCookie = function(a, b, c) {
    var d = new Date();
    c ? d.setTime(d.getTime() + c) : d.setTime(d.getTime() + _a_month), document.cookie = a + "=" + escape(b) + ";path=/;expires=" + d.toGMTString();
}, getCookie = function(a) {
    var b, c = new RegExp("(^| )" + a + "=([^;]*)(;|$)");
    return b = document.cookie.match(c), b ? unescape(b[2]) : null;
}, delCookie = function(a) {
    var b = getCookie(a);
    b && (document.cookie = a + "=" + b + ";expires=" + new Date(0).toGMTString());
};

//common
var txtToJson = function(txt) {
    var j = {};
    if (txt) try {
        j = JSON.parse(txt);
    } catch (e) {
        try {
            j = eval("(" + txt + ")");
        } catch (ee) {}
    }
    return j;
}, jsonToTxt = function(obj) {
	return JSON.stringify(obj);
}, isString = function(_str) {
    return typeof _str == "string" || _str && typeof _str.substr == "function";
}, getQueryString = function(name) {
    var reg = new RegExp("(^|&)" + name + "=([^&]*)(&|$)", "i");
    var r = window.location.search.substr(1).match(reg);
    if (r != null) return decodeURI(r[2]);
    return null;
},
formatUrl = function(data, url){
	var param_url = '';
	if(data){
		var count = 0;
		for(var i in data){
			if(count <= 0){
				param_url = param_url +i+'='+data[i];	
				count ++;
			}else{
				param_url = param_url + '&' +i+'='+data[i];	
			}
			
		}
	}
	if(url && url.length > 0){
		if(url.indexOf('?') > 0){
			param_url = url + '&' + param_url;
		}else{
			param_url = url + '?' + param_url;
		}
	}
	console.log('> formatUrl :: param_url :: '+param_url);
	return param_url;
	
},
scroll_length = 0,
scrollToTop = function(length){
	/*try{
		scrollBy(0,-$(window).scrollTop());	
	}catch(err){}*/
//	window.scrollBy(0,-5);	//Only for y vertical-axis
//	if(document.body.scrollTop>0) { 
//	  	sdelay = setTimeout('scrollToTop()',10);
//	}

	window.scrollBy(0,-5);	//Only for y vertical-axis
	scroll_length = scroll_length+5;
	if(typeof length != 'undefined'){
		 if (length > scroll_length) {
		 	sdelay = setTimeout(function(){
		 		scrollToTop(length);
		 	},10);
		 }else{
		 	scroll_length = 0;
		 }
	}else{
		if(document.body.scrollTop>0) { 
		  	sdelay = setTimeout('scrollToTop()',10);
		}
	}
},
scrollToBottom = function(length){
	window.scrollBy(0,5);	//Only for y vertical-axis
	scroll_length = scroll_length+5;
	if(typeof length != 'undefined'){
		 if (length > scroll_length) {
		 	sdelay = setTimeout(function(){
		 		scrollToBottom(length);
		 	},10);
		 }else{
		 	scroll_length = 0;
		 }
	}else{
		if($("body").height() > ($(window).height() + $(window).scrollTop())) { 
		  	sdelay = setTimeout('scrollToBottom()',10);
		}
	}
},
showLoading = function(flag){
	if($('#J_loading').length <= 0) return;
	if($('#J_loading').html().length <= 0){
		$('#J_loading').html('<img src="http://img.kuaidadi.com/cms/img/7bb1629871ca14562b1aaf1ca9e194f3.gif" style="position:relative;top:2px;"/>&nbsp;...');
	}
	if(flag){
		$('#J_loading').show();	
	}else{
		$('#J_loading').hide();	
	}
},
check_json_result = function(e){
	if(typeof e == 'undefinded'){
		console.log('> json result is undefinded.');
		return false;
	}else if(!e){
		console.log('> json result is empty.');
		return false;
	}
	if(typeof e == 'object'){
		e=jsonToTxt(e);
//		console.log("e::"+e);
	}
	if(e.indexOf(':9001}') > 0){
		console.log('> json result is 9001.');
		return false;
	}else{
		return true;	
	}
},
/*
 * url idxtoken root url
 * f_root falseroot
 */
fetchUrl = function(url, f_root){
	var _idx = $('#idx').val(),_token = $('#token').val(),_root = $('#root').val();
	if(typeof _idx == 'undefined'){
		_idx = '';
	}
	if(typeof _token == 'undefined'){
		_token = '';
	}
	if(typeof _root == 'undefined' || (typeof f_root != 'undefined' && !f_root)){
		_root = '';
	}
	
	var _ver = getQueryString('ver')?getQueryString('ver'):'';
	var _locole = getQueryString('locale')?getQueryString('locale'):'';
	var _strParams = '&ver='+_ver+'&locale='+_locole;

	if (url.indexOf('?')>0) {
		return _root + url + '&idx='+_idx+'&token='+_token+_strParams;
	}else{
		return _root + url + '?idx='+_idx+'&token='+_token+_strParams;
	}
},
/*
 * $.trim();
 */
str_trim = function(str){
	return str.replace(/\s+/g,"");	
},
error_callback = function(e){
	toast('');
},
/*
 * 2
 */
sub_string = function(str, length){
	var sub_length = 80;
	if (typeof length != 'undefined' && length > 0) {
		sub_length = length;
	}
	var temp1 = str.replace(/[^\x00-\xff]/g,"**");//
	var temp2 = temp1.substring(0,sub_length);
	//*
	var x_length = temp2.split("\*").length - 1 ;
	var hanzi_num = x_length /2 ;
	sub_length = sub_length - hanzi_num ;//sub-
	var res = str.substring(0,sub_length);
	if(sub_length < str.length ){
		var end =res+"" ;
	}else{
		var end = res ;
	}
	return end ;
}

//,iphone
var isIPHONE = navigator.userAgent.toUpperCase().indexOf('IPHONE') != -1;
var objBlur = function(id, time) {
//	console.log("> call objBlur.id::"+id);
    if (typeof id != 'string') console.log('objBlur()');
    var obj = document.getElementById(id),
    time = time || 300,
    docTouchend = function(event) {
        if (event.target != obj) {
            setTimeout(function() {
                obj.blur();
                document.removeEventListener('touchend', docTouchend, false);
            },
            time);
        }
    };
    if (obj) {
        obj.addEventListener('focus',
        function() {
            document.addEventListener('touchend', docTouchend, false);
        },
        false);
    } else {
//      throw new Error('objBlur()');
		console.log('objBlur()');
    }
},
createObjBlur = function(){
	if (isIPHONE) {
		$('input').each(function(i,ele){
			var _type = $(ele).attr('type');
			var _id = $(ele).attr('id');
			if(_type != 'hidden' && typeof _id != 'undefined'){
//				console.log("id>"+_id);
				var input = new objBlur(_id);
				input = null;
			}
		})
	}
}

var sdelay=0;

$(function() {
    document.documentElement.style.webkitTouchCallout = "none";
    //document.documentElement.style.webkitUserSelect = "none";
});document.addEventListener('DOMContentLoaded', function() {
    (function(window, undefined) {

        // dd.dialogdd.base
        // dialogdd
        var dd = window.dd || {};
        if (dd.dialog) return dd.dialog;


        var d = {};

        var docElem = document.documentElement,
            docEleClientH = docElem.clientHeight,
            docEleClientW = docElem.clientWidth;

        var dvWall = null,
            dvWrap = null,
            dialog = null;
        /**
         * 
         */
        var isArray = function(obj) {
            return (typeof Array.isArray) ? Array.isArray(obj) : (Object.prototype.toString.call(obj) === '[object Array]');
        };

        /**
         * bodydiv
         */
        var insertDom = function(newNode) {
            document.body.appendChild(newNode);
        };
        /**
         * util
         */
        var util = {
            /**
             * dom
             */
            genDom: function(opts, div_wall, div_wrap) {
                if (!opts) return;

                if (Object.prototype.toString.call(opts, null) === '[object Object]') { // object
                    opts.type = opts.type || "loading";
                    opts.bg = opts.bg || "";
                    opts.op = opts.op || "";

                    opts.width = opts.width || "280px";
                    opts.d_bg = opts.d_bg || "#fff";
                    opts.d_op = opts.d_op || "";
                    //alert(opts.d_bg);
                    //&wrap
                    opts.wallCss = "background:" + opts.bg + ";opacity:" + opts.op + ";filter:" + opts.filter + ";";
                    opts.wrapCss = "text-align:center;width:" + opts.width + ";height:" + opts.height + ";opacity:" + opts.d_op + ";filter:" + opts.d_filter + ";" + "background: " + opts.d_bg + ";";

                    div_wall.style.cssText = opts.wallCss;
                    div_wrap.style.cssText = opts.wrapCss;

                    //
                    var html = "<div style='" + ((opts.type === 'loading') ? "padding:0px;" : "padding: 0px 6%;") + "'>" + util.genIcon(opts) + util.genTitle(opts) + util.genTip(opts) + util.genButtons(opts) + "</div>" + util.genClose(opts);
                    div_wrap.innerHTML = html;

                } else if (Object.prototype.toString.call(opts, null) === '[object String]') { // html
                    div_wrap.innerHTML = opts;
                } else if (Object.prototype.toString.call(opts, null) === '[object HTMLDivElement]') { // dom
                    div_wrap.style.cssText = "display:inline-block;width:280px;background-color:#fff;";
                    opts.style.display = "inline-block";
                    div_wrap.appendChild(opts);
                }

            },
            /**
             * iconhtml
             */
            genIcon: function(opts) {
                //icon,trueicon
                if (!opts.icon) {
                    return "";
                }
                var res = "";
                opts.icon = opts.icon || {};
                var w = opts.icon.width || "8px",
                    h = opts.icon.height || "36px",
                    url = opts.icon.url || "http://static.diditaxi.com.cn/webapp/images/i-plaint.png";
                var margin = "",
                    icon = "",
                    cssText = "";
                if (opts.type === 'loading') {
                    url = opts.icon.url || "http://static.xiaojukeji.com/webapp/images/loading_2.gif"
                    margin = "margin:36px 0 10px";
                    cssText = 'display:inline-block;width:30px;height:30px;background:url(' + url + ') no-repeat;background-size:30px 30px;';

                } else {
                    margin = "margin:24px 0 12px";
                    cssText = 'display:inline-block;width:60px;height:60px;background-color:#f0f0f0;background-size:60px 60px;border-radius:50%;'
                    icon = '<span style="vertical-align:middle;display:inline-block;height:100%;"></span><img src=' + url + ' style="width:' + w + ';height:' + h + ';vertical-align:middle;" />';
                }
                res = '<p  style="' + margin + '"><span style="' + cssText + '">' + icon + '</span></p>';
                return res;
            },

            /**
             * TitleHTML
             */
            genTitle: function(opts) {
                opts.title = opts.title || {};
                var color = opts.title.color || "#333",
                    size = opts.title.size || "16px";
                return '<p class="font-bold" style="margin-bottom: 5px;line-height:40px;color:' + color + ';font-size:' + size + ';">' + (opts.title.txt || "") + '</p>';
            },

            /**
             * 
             */
            genTip: function(opts) {
                if (opts.title && opts.title.txt) {
                    opts.tip.color = opts.tip.color || "#666";
                    opts.tip.size = opts.tip.size || '14px'
                } else {
                    opts.tip.color = opts.tip.color || "#333";
                    opts.tip.size = opts.tip.size || '16px';
                }
                return opts.tip ? '<p style="' + ((opts.type !== "loading") ? "text-align:center;" : "") + 'line-height:32px;color:' + opts.tip.color + ';font-size:' + opts.tip.size + ';">' + opts.tip.txt + '</p>' : "";
            },
            /**
             * 
             */
            genClose: function(opts) {
                return opts.close ? '<a class="d-close" href="javascript:void(0);"></a>' : '';
            },
            /**
             * 
             */
            genButtons: function(opts) {
                var res = "";
                if (opts.btns && isArray(opts.btns)) {
                    res += '<div style="padding:20px 0;">';
                    for (var i = 0, l = opts.btns.length; i < l; i++) {
                        var btn = opts.btns[i];
                        if (btn && opts.type === 'alert') {
                            res += '<a class="' + btn.kls + '" id="' + btn.id + '">' + btn.val + '</a>';
                        } else if (btn && opts.type === 'confirm') { // 
                            res += '<a class="' + btn.kls + '" id="' + btn.id + '" style="width: 43%; height: 40px; line-height: 40px; margin:0 3%;">' + btn.val + '</a>';
                        } else { //
                            res += '<a class="' + btn.kls + '" id="' + btn.id + '" style="margin:5px 0;">' + btn.val + '</a>';
                        }
                    }
                    res += '</div>';
                }
                //
                if (opts.ext && typeof opts.ext === 'string') {
                    res += opts.ext;
                }
                return res;
            },

            /**
             * 
             */
            addEvents: function(opts) {
                var btn = null;
                if (opts.close) {
                    var close = document.getElementsByClassName("d-close")[0];
                    close.addEventListener("click", function() {
                        dialog.hide();
                    }, false);
                }
                if (!isArray(opts.btns) || !opts.btns.length) return;

                for (var i = 0, l = opts.btns.length; i < l; i++) {
                    btn = opts.btns[i];
                    if (btn) {
                        var ev = btn.event || "click",
                            ele = document.getElementById(btn.id);
                        if (ele) {
                            ele.removeEventListener(ev, btn.handler, false);
                            ele.addEventListener(ev, btn.handler, false);
                        }
                    }
                }
            }
        };

        /**
         * Dialognew
         */
        var Dialog = function(opts) {
            if (!(this instanceof Dialog)) {
                dialog = new Dialog(opts);
                return dialog; // newdialog.fn.init,return
            } else {
                new Dialog.fn.init(opts);
            }
        };

        /**
         * Dialog prototype
         * @type {Function}
         */
        Dialog.fn = Dialog.prototype = {
            constructor: Dialog,
            init: function(opts) {
                if (!opts) return;

                var div_wall = document.createElement('div');
                var div_wrap = document.createElement("div");
                div_wall.id = "d-wall";
                div_wrap.id = "d-wrap";

                // HTML
                util.genDom(opts, div_wall, div_wrap);

                //
                dvWall && document.body.removeChild(dvWall);
                dvWrap && document.body.removeChild(dvWrap);

                //dom
                insertDom(div_wall);
                insertDom(div_wrap);

                dvWall = div_wall;
                dvWrap = div_wrap;

                if (Object.prototype.toString.call(opts, null) === '[object Object]') {
                    window.setTimeout(function() {
                        util.addEvents(opts);
                    }, 400);
                }
            },
            show: function() {
                var that = this;
                if (dvWall && dvWrap) {
                    that.reset();
                    dvWall.style.display = "block";
                    dvWrap.style.display = "inline-block";

                    window.addEventListener("resize", reset, false);
                    window.addEventListener("scroll", reset, false);

                    function reset(event) {
                        window.removeEventListener(event.type, reset, false); //remove event
                        that.reset.call(that);
                    }
                }
            },
            hide: function() {
                if (dvWall && dvWrap) {
                    dvWall.style.display = "none";
                    dvWrap.style.display = "none";
                }
            },
            reset: function() {
                if (dvWall && dvWrap) {
                    dvWrap.style.top = (docEleClientH - dvWrap.clientHeight - 20) / 2 + "px";
                    dvWrap.style.left = (docEleClientW - dvWrap.clientWidth) / 2 + "px";
                    var scrollH = document.body.scrollHeight || document.documentElement.scrollHeight; //
                    dvWall.style.width = docEleClientW + "px";
                    dvWall.style.height = scrollH + "px";
                }
            }
        };


        /**
         * alert
         */
        d.alert = function(cfg) {
            var opts = {};
            if (typeof arguments[0] === "string" && arguments[0]) {
                opts.title = arguments[1] || "";
                opts.tip = arguments[0];
                opts.btn = {
                    val: arguments[2] || ""
                };
            } else if (cfg && typeof cfg === 'object') {
                opts = cfg;
            }
            
            var btn = opts.btn || {};
            
            dialog = Dialog({
                type: "alert",
                icon: opts.icon || null,
                title: {
                    txt: opts.title||""
                },
                tip: {
                    txt: opts.tip
                },
                btns: [{
                    id: "btn-close",
                    kls: 'btn-green',
                    event: "click",
                    val: (opts.btn && opts.btn.val) || "",
                    handler: function(ev) {
                        dialog.hide();
                        if (typeof btn.handler === 'function') {
                            btn.handler(ev);
                        }
                    }
                }]
            });
            dialog.show();
        };

        /**
         * confirm dialog
         */
        d.confirm = function(cfg) {
            var opts = {};

            if (typeof arguments[0] === 'string' && arguments[0]) {
                opts.text = arguments[0] || "";
                opts.confirm = {};
                opts.confirm.handler = arguments[1];

            } else if (cfg && typeof cfg === 'object') {
                opts = cfg;
            }

            var cancel = opts.cancel || {};
            var confirm = opts.confirm || {};

            dialog = Dialog({
                type: "confirm",
                tip: {
                    txt: opts.text
                },
                icon: opts.icon || {
                    url: "http://static.diditaxi.com.cn/webapp/images/i-plaint.png"
                },
                btns: [{
                    id: cancel.id || "btn-cancel",
                    val: cancel.val || "",
                    kls: cancel.kls || "btn-white",
                    event: cancel.event || "click",
                    handler: function(e) {
                        dialog.hide();
                        if (typeof cancel.handler === 'function') {
                            cancel.handler(e);
                        }
                    }
                }, {
                    id: confirm.id || "btn-ok",
                    val: confirm.val || "",
                    kls: confirm.kls || "btn-orange",
                    event: confirm.event || "click",
                    handler: function(e) {
                        dialog.hide();
                        if (typeof confirm.handler === 'function') {
                            confirm.handler(e);
                        }
                    }
                }],
                ext: opts.ext
            });
            dialog.show();
        };

        /**
         * Loading Dialog
         */
        d.loading = function(cfg) {
            var opts = {};
            if (typeof arguments[0] !== "object") {
                opts.text = arguments[0];
                opts.time = arguments[1] || 0
            } else {
                opts = cfg;
            }
            dialog = Dialog({
                type: "loading",
                bg: "#fff",
                d_bg: "#0c0d0d",
                d_op: "0.7",
                width: "140px",
                height: "140px",
                icon: (cfg && cfg.icon)||{},
                tip: {
                    txt: opts.text || "",
                    color: "#fff",
                    size: "14px"
                }
            });

            dialog.show();

            if (!opts.time) {
                opts.time = 5000;
            }
            window.setTimeout(function() {
                dialog.hide();
                console.log(typeof opts.hideCB === 'function')
                if (typeof opts.hideCB === 'function') {
                    opts.hideCB();
                }
            }, opts.time);
        };

        /**
         * loading
         */
        d.flatLoading = function(cfg) {
            var opts = {};
            if (typeof arguments[0] !== "object") {
                opts.text = arguments[0];
                opts.time = arguments[1] || 0
            } else {
                opts = cfg;
            }
            dialog = Dialog({
                type: "loading",
                bg: "#fff",
                op: '1',
                d_bg: "#fff",
                d_op: "1",
                width: "140px",
                height: "140px",
                icon: true,
                tip: {
                    txt: opts.text || "",
                    color: "#666",
                    size: "14px"
                }
            });

            dialog.show();

            if (!opts.time) {
                opts.time = 5000;
            }
            window.setTimeout(function() {
                dialog.hide();
                if (typeof opts.hideCB === 'function') {
                    opts.hideCB();
                }
            }, opts.time);
        };

        /**
         * logoloading
         */
        d.logoLoading = function(time, hideCB) {
            dialog = Dialog('<div class="loading-car"><div class="bg"></div><div class="loading-car-icon"></div></div>');
            dialog.show();
            if (!time) {
                time = 5000;
            }
            setTimeout(function() {
                dialog.hide();
                if (typeof hideCB === 'function') {
                    hideCB();
                }
            }, time);
        };

        /** 
            cfg: {
                title: "",
                content: "",
                okFn: fn,
                cancel: fn
            }
         */
        d.guideUdache = function(cfg) {
            if (!cfg) return;

            cfg.title = cfg.title || "";
            cfg.content = cfg.content || "";
            cfg.cancelVal = cfg.cancelVal || "";
            cfg.confirmlVal = cfg.confirmlVal || "";
            cfg.image_url = "http://static.xiaojukeji.com/webapp/images/ad_biz.png";
            var dialog = Dialog({
                type: "confirm",
                tip: {
                    txt: "<span style='display:block;font-size:2.3rem;color:#ff8903;line-height:30px;margin-top:160px' >" + cfg.title + "</span><span style='color:#666;font-size:1.2rem;' >" + cfg.content + "</span>"
                },
                d_bg: "url(" + cfg.image_url + ") no-repeat #fff;background-size:100% auto;",
                height: "auto",
                close: cfg.close || false,
                btns: [{
                    id: "btn-cancel",
                    val: cfg.cancelVal,
                    kls: "btn-white",
                    event: "click",
                    handler: function(e) {
                        dialog.hide();
                        cfg.cancelFn && cfg.cancelFn();
                    }
                }, {
                    id: "btn-ok",
                    val: cfg.confirmlVal,
                    kls: "btn-orange",
                    event: "click",
                    handler: function(e) {
                        dialog.hide();
                        cfg.okFn && cfg.okFn();
                    }
                }]
            });
            dialog.show();
        };

        /**
            cfg: {
                clz:
                text: "",
                time: 2000,
                
            }
        */
        d.tip = function(cfg) {
            var _cfg = {};
            if (typeof arguments[0] !== "object") {
                _cfg.text = arguments[0];
                _cfg.time = arguments[1] || 0
            } else {
                _cfg = cfg;
            }
            _cfg.time = parseInt(_cfg.time) || 600;


            dialog = Dialog({
                type: "tip",
                bg: "#fff",
                d_bg: "#0c0d0d",
                d_op: "0.7",
                width: "140px",
                height: "140px",
                icon: _cfg.icon || {
                    url: "http://static.diditaxi.com.cn/webapp/images/i-plaint.png",
                    width: "8px",
                    height: "36px"
                },
                tip: {
                    txt: _cfg.text || "",
                    color: "#fff",
                    size: "14px"
                }
            });

            dialog.show();

            window.setTimeout(function() {
                dialog.hide();
            }, _cfg.time);
        };

        d.Fn = Dialog;

        window.dd = dd; // ddwindow
        dd.dialog = d; // ddialog

    })(window);
}, false);
/**
 * Created by chenmingyang on 15/6/23.
 */
;(function($){
    var Utils = {
        //mainPageUrl:'http://115.182.212.196:9006/api/v2/weixinapi',
        mainPageUrl:'http://pay.xiaojukeji.com/api/v2/weixinapi?page=orderinfo&biz=4',
        reLogin:function(err,msg){
            var opts = {};
            opts.tip = msg;
            opts.btn={
                val:'',
                handler:function(){
                    location.replace(Utils.mainPageUrl);
                }
            };
            dd.dialog.alert(opts);
        },
        getHash:function(){
            var search = {};
            if(location.search){
                var SEARCH = location.search.split('?')[1].split('&');
                SEARCH.forEach(function(item){
                    search[item.split('=')[0]] = item.split('=')[1];
                });
            }
            search.token = search.token?decodeURIComponent(search.token):'';
            search.daijia_token = search.daijia_token?decodeURIComponent(search.daijia_token):'';
            return search;
        },
        formatDate:function (date, format) {
            format = format || 'yyyy-MM-dd hh:mm:ss';
            if(!date){
                return '';
            }
            var now = new Date(date);
            var o = {
                "M+": now.getMonth() + 1, //month
                "d+": now.getDate(), //day
                "h+": now.getHours(), //hour
                "m+": now.getMinutes(), //minute
                "s+": now.getSeconds(), //second
                "q+": Math.floor((now.getMonth() + 3) / 3), //quarter
                "S": now.getMilliseconds() //millisecond
            }
            if (/(y+)/.test(format)) {
                format = format.replace(RegExp.$1, (now.getFullYear() + "")
                    .substr(4 - RegExp.$1.length));
            }
        
            for (var k in o) {
                if (new RegExp("(" + k + ")").test(format)) {
                    format = format.replace(RegExp.$1,
                        RegExp.$1.length == 1 ? o[k] :
                        ("00" + o[k]).substr(("" + o[k]).length));
                }
            }
            return format;
        }
    };
    if(!window.KUI){
        window.KUI = {};
    }
    KUI.Utils = Utils;
})(Zepto);$(function() {
    var kop = new KUI.KOP();

    // var base = dd.base || {},
         dialog = dd && dd.dialog;

    var did = getQueryString("daijia_did") || getQueryString("did"),
    	pid = getQueryString("daijia_pid") || getQueryString("pid"),
    	oid = getQueryString("daijia_oid") || getQueryString("oid"),
    	token = getQueryString("daijia_token") || getQueryString("token"),
        showCount = getQueryString("showCount") || 1;
    var sheight = document.documentElement.clientHeight;
    $("html,body").css("min-height",sheight+"px");

    var J_resasons = document.getElementsByClassName('J_resasons'),
        J_btn_submit = document.getElementById('J_btn_submit'),
        J_reason = document.getElementById("J_reason");

    function init() {
        fetchDriverDetail();
    }

    function fetchDriverDetail() {
        var _temp = '<div class="driver-face"><img src="{@if headTitleUrl}$${headTitleUrl}{@else}http://img.kuaidadi.com/cms/img/df3ab959ef2642722debac84ab5d624e.png{@/if}" alt="$${name}"></div>' + '<div class="driver-name">' + '<p class="name">$${name}</p>' +'<p class="desc">$${driveringCount}$${driveringAge}</p>' + '<div class="driver-cont">' + '$${starLevel_html}' + '</div>';
        //     		dialog.loading();
        // API 
        kop.send('lj.c.p.d.driverInfo', {
            'did': did,
            'oid':oid
        }, pid, token, function(data) {
            var data = data.driverInfo;
            if (showCount == 1){
                data.driveringCount = ""+data.driveringCount+""
            }else {
                data.driveringCount = "";
            }
            data.starLevel_html = fetch_satars_html(data.starLever,false);
            //				data.name = data.name.substr(0, 1)+'';
            //driver service count
            var _html = juicer(_temp, data);
            $('.driver-main').html(_html);
            // var _temp_service = '<span><p>$${driveringCount}</p><p></p></span><span class="ui-border-l"><p>$${driveringCount}</p><p></p></span>';
            // if (showCount != 1) {
            //     _temp_service = '<span style="width:99%;"><p>$${driveringAge}</p><p></p></span>';
            // }
            // var _html = juicer(_temp_service, data);
            //$('.driver-service').html(_html);
            //driver tags
            // _temp_impress =
            //     '{@if driverTags && driverTags.length > 0}'+ '<section class="container-title"><div class="midline-l"></div><div class="line-title"></div><div class="midline-r"></div></section>'+ '{@each driverTags as it}<div class="ui-border-radius">$${it.tagTitle}&nbsp;&nbsp;<em>$${it.tagCount}</em></div>{@/each}{@/if}';
            var _temp_impress = $('#J_Impress_Tpl').html();
            var _html = juicer(_temp_impress, data);
            $('.driver-impress').html(_html);

            var _temp_pictiure = '<img src=${identityUrl}><img src=${driverCarUrl}>';
            $('.driver-picture').html(juicer(_temp_pictiure,data));
            //logo
            if(data.driverTags.length==0&&data.evaluationContentList.length==0){
                $('section.logo').hide();
            }

            fetch_comments_html(data);

        }, function(err, msg) {
            console.log(err + ';' + msg);
            dialog.alert(msg.substr(0, 100));
        });
    }

    function fetch_satars_html(level,needtrue) {
        level ? '' : level = 5;
        if (!needtrue) {
            level < 4 ? level = 4 : '';
        };
        var _int = parseInt(level);
        var _d = level - _int;
        var _strStars = '';

        if (_int > 0) {
            for (var i = 0; i < _int; i++) {
                _strStars += '<i class="icon iconfont icon-star on"></i>';
            }
        }
        if (_d > 0) {
            _strStars += '<i class="icon iconfont icon-star"><i class="icon iconfont icon-star half"></i></i>';

        }
        if (_int < 5) {
            var _sub;
            if (_d > 0) {
                _sub = 4-_int;
            }else{
                _sub = 5-_int;
            };
            for (var i = 0; i < _sub; i++) {
                _strStars += '<i class="icon iconfont icon-star"></i>';
            }
        }

        return _strStars;
    };

    function fetch_comments_html(data){
    	$.each(data.evaluationContentList,function(index,item){
    		var commnetImpressionList = item.commnetImpressionList;
    		//commnetImpressionList.splice(4);
    		item.star = fetch_satars_html(item.commentStar,true);
    		item.date = KUI.Utils.formatDate(item.date,"yyyy-MM-dd hh:mm");
    		// if(!commnetImpressionList.length&&item.commnetContent==''){

    		// }
    	});
    // 	var tmp = '{@if evaluationContentList && evaluationContentList.length > 0}'+
    //     '<section class="container-title"><div class="midline-l"></div><div class="line-title"></div><div class="midline-r"></div></section>'+
    //     '{@each evaluationContentList as item}<div class="comment-item ui-border-b">' +
				//     '<div class="comment-detail">' +
				//         '<div class="detail">' +
				//             '<span class="date">${item.date}</span>' +
				//         '</div>' +
				//         '<div class="driver-cont">$${item.star}</div>' +
				//     '</div>' +
				//     '<div class="comment-content">${item.commnetContent}</div>' +
    //                 '<div class="comment-lable">{@each item.commnetImpressionList as lable}<div class="lables ui-border-radius">$${lable}</div>{@/each}</div>'+
				// '</div>{@/each}{@/if}';
        var tmp = $('#J_Tpl').html();
		//var temp2 = '<div class="comment-lable">{@each item.commnetImpressionList lable}<div class="lables">$${lable}</div>{@/each}</div>';
		$('.driver-comments').html(juicer(tmp, data));
    };

    init();
});
