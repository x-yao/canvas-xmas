    		var _self = this;
    		if(window.ga){
    			_self._send(cfg);
    		}else{
    			_self._gaList.push(cfg);
    			// timer
    			if(!_self._gaTimer){
    				_self._gaTimer = window.setInterval(_self._gaHandler(), _self.config.gaTimerInterval);
    			}
    		}
   	
    	},
    	_gaHandler: function(){
    		var _self = this;
    		return function(){
				if(ga){
					// timer
					window.clearInterval(_self._gaTimer);
					_self._gaTimer = null;
					// ga
					$.each(_self._gaList, function(i, gaItem){
						_self._send(gaItem);
					});
				}	
    		}
    	},
    	// 
        _send:function(cfg){
            var _self = this,
            	config = $.extend({
					category: 'button',
					action: 'click',
					label: 'default',
					value: 1
            	}, cfg);
			ga && ga('send', 'event', config.category, config.action, config.label, config.value);
        }
        
    });

    if(!window.KUI){
        window.KUI = {};
    }
    KUI.Analytics = Analytics;

})(Zepto);
